//na
#include<bits/stdc++.h>
using namespace std;
#define ll long long int
int main(){
    int t = 0;
    cin >> t;
    while (t--){
        int n,x; 
        cin >> n >> x; 
        ll tmp; 
        int vt = -1; 
        for (int i = 0; i<n; i++){
            cin >> tmp; 
            if ( tmp <= x) vt = i+1; 
        }
        cout << vt << endl; 
    }
    return 0;
}
