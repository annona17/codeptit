//na
#include<bits/stdc++.h>

using namespace std;
#define ll long long int
bool comp( const int a, const int b){
    return a>b; 
}
int main(){
    int t = 0;
    cin >> t;
    while (t--){
        int n, k; 
        cin >> n >> k; 
        int a[100005] = {}; 
        for (int i = 0; i<n; i++) cin >> a[i]; 
        sort(a, a+n, comp);
        for (int i = 0; i<k; i++) cout << a[i] << " "; 
        cout << endl; 
    }
    return 0;
}
