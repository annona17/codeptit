//na
#include<bits/stdc++.h>

using namespace std;
#define ll long long int
int test(int a[], int n, int x ){
    for (int i = 0; i< n; i++){
        if( a[i] == x) return 1; 
    }
    return -1; 
}
int main(){
    int t = 0;
    cin >> t;
    while (t--){
        int n, x; 
        cin >> n >> x; 
        int a[1000005] = {}; 
        for (int i = 0; i< n; i++) cin >> a[i]; 
        cout << test(a,n,x) << endl; 
    }
    return 0;
}
