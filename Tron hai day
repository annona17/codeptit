//na
#include<bits/stdc++.h>

using namespace std;
#define ll long long int

int main(){
    int t = 0;
    cin >> t;
    while (t--){
        int n = 0, m = 0, tmp; 
        cin >> n >> m; 
        vector <int> a; 
        for (int i = 0; i< n; i++) {
            cin >> tmp; 
            a.push_back(tmp); 
        }
        for (int i = 0; i< m; i++) {
            cin >> tmp; 
            a.push_back(tmp); 
        }
        sort(a.begin(), a.end()); 
        for (auto x: a) cout << x << " "; 
        cout << endl; 
    }
    return 0;
}
